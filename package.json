{
  "name": "ze-benchmarks",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "pnpm --filter ze-evaluator run build && pnpm --filter @ze/harness run build",
    "build:evals": "pnpm --filter ze-evaluator run build",
    "build:adapters": "pnpm --filter @ze/agent-adapters run build",
    "dev": "pnpm --filter @ze/harness run dev",
    "bench": "tsx packages/harness/src/cli.ts",
    "harness": "tsx packages/harness/src/cli.ts",
    "start:report": "pnpm --filter benchmark-report run dev",
    "bench:run": "tsx scripts/run-all-models.ts",
    "bench:compare": "tsx scripts/compare-batches.ts",
    "clear:db": "tsx scripts/clear-database.ts"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/figlet": "^1.7.0",
    "next": "^14.2.0",
    "tsx": "^4.20.6",
    "vitest": "^2.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "core-js",
      "esbuild"
    ]
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "chalk": "^5.6.2",
    "cli-progress": "^3.12.0",
    "dotenv": "^16.4.5",
    "figlet": "^1.9.3",
    "json5": "^2.2.3",
    "yaml": "^2.8.1"
  }
}