{
  "name": "ze-benchmarks-worker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "dev:local": "wrangler dev --env local",
    "dev:remote": "wrangler dev --remote",
    "deploy": "wrangler deploy",
    "deploy:dev": "wrangler deploy --env dev",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "db:generate": "drizzle-kit generate",
    "db:check": "tsx scripts/check-db.ts",
    "db:check:remote": "tsx scripts/check-db.ts --remote",
    "db:migrate:local": "wrangler d1 migrations apply ze-benchmarks-local --local",
    "db:migrate:dev": "wrangler d1 migrations apply ze-benchmarks-dev --env dev --remote",
    "db:migrate:staging": "wrangler d1 migrations apply ze-benchmarks-staging --env staging --remote",
    "db:migrate:production": "wrangler d1 migrations apply ze-benchmarks-prod --env production --remote",
    "db:studio": "drizzle-kit studio",
    "db:sync-from-prod": "./scripts/sync-from-production.sh",
    "sync": "tsx scripts/sync.ts"
  },
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "drizzle-orm": "^0.36.0",
    "itty-router": "^4.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "core-js",
      "esbuild"
    ]
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240000.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.14.10",
    "drizzle-kit": "^0.28.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "wrangler": "^4.46.0"
  }
}
